#!/bin/sh
# bin/ecl-config.pre.  Generated from ecl-config by configure.

usage()
{
  cat <<EOF
Usage: $0 [OPTIONS] [LIBS]
Options: 
	[--cflags]
	[--libs|--ldflags]
Libs:
	cmp
EOF
}

LDFLAGS="-lecl"
for i in $*; do
  case $i in
    --cflags|-c)
      echo_cflags=yes
      ;;
    --libs|--ldflags|-l)
      echo_ldflags=yes
      ;;
    cmp)
      LDFLAGS="$LDFLAGS -lcmp"
      ;;
    *)
      usage 1 >&2
      ;;
    esac;
done

if test "$echo_cflags" = "yes"; then
  echo "-Dlinux   -I/home/linkfly/Downloads/lisp-dev-tools//home/linkfly/Downloads/lisp-dev-tools/lisp/ecl/ecl-11.1.1/include/"
fi

if test "$echo_ldflags" = "yes"; then
  echo "-Wl,--rpath,/home/linkfly/Downloads/lisp-dev-tools//home/linkfly/Downloads/lisp-dev-tools/lisp/ecl/ecl-11.1.1/lib/ -L/home/linkfly/Downloads/lisp-dev-tools//home/linkfly/Downloads/lisp-dev-tools/lisp/ecl/ecl-11.1.1/lib/ $LDFLAGS   -ldl  -lm "
fi
