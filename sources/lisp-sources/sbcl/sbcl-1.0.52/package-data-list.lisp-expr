               ;; convenience on all platforms.
               "GENERATION-AVERAGE-AGE"
               "GENERATION-BYTES-ALLOCATED"
               "GENERATION-BYTES-CONSED-BETWEEN-GCS"
               "GENERATION-MINIMUM-AGE-BEFORE-GC"
