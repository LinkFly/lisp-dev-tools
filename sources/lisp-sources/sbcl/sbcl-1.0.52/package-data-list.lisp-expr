               ;; Not an atomic operation, but should be used with them
               "SPIN-LOOP-HINT"

               ;; Time related things
               "CALL-WITH-TIMING"
               "GET-TIME-OF-DAY"

               ;; People have various good reasons to mess with the GC.
               "*AFTER-GC-HOOKS*"
               "BYTES-CONSED-BETWEEN-GCS"
               "GC" "GET-BYTES-CONSED"
               "*GC-RUN-TIME*"
               "PURIFY"
