               ;; nonstandard but very useful.
               "*POSIX-ARGV*" "*CORE-PATHNAME*" "*RUNTIME-PATHNAME*"
               "POSIX-GETENV" "POSIX-ENVIRON"

               ;; Customizing initfile locations
               "*USERINIT-PATHNAME-FUNCTION*"
